<Window x:Class="VisualisationHeuristique.Visualisation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualisationHeuristique"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700">
    <DockPanel>
        <ComboBox DockPanel.Dock="Top" Width="300" x:Name="typeAffichage" SelectionChanged="typeAffichage_SelectionChanged" />
        <ComboBox DockPanel.Dock="Top" Width="300" x:Name="selectFile" SelectionChanged="selectFile_SelectionChanged" />
        <DockPanel x:Name="grapheContainer" Loaded="grapheContainer_Loaded" Panel.ZIndex="-500" />

    </DockPanel>
</Window>
